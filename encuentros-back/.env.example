# Configuración de Base de Datos Oracle
DB_TYPE=oracle
DB_HOST=localhost
DB_PORT=1521
DB_USERNAME=C##ENCUENTROS_ADMIN
DB_PASSWORD=admin
DB_SERVICE_NAME=XE
# O usa DB_SID en lugar de DB_SERVICE_NAME según tu configuración
# DB_SID=XE

# Configuración de JWT
# IMPORTANTE: En producción, usa una clave secreta fuerte y única
# Genera una con: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=encuentros_secret_key_2025_change_this_in_production

# Puerto del servidor
PORT=3000

# Entorno de ejecución
NODE_ENV=development
